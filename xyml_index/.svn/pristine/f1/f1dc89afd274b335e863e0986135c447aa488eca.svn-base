/* pages/new_order/new_order.wxss */
 
page{background: #f6f6f6;}
.menu{width: 100%; height: 90rpx; line-height: 90rpx; text-align: center; color: #333; font-size:28rpx; background: #fff; position: fixed;left: 0px;top: 0px;border-bottom: 1px solid #eee; z-index: 9;}
.menu view{width:50%;float: left;position: relative;}
.menu .on{ color: #dc0531; }
.menu .on:after{ width:60%; height:2px; content:""; display:block; position:absolute; left:20%; bottom:0px;background: #dc0531; border-radius:45px; -moz-border-radius:45px;}

.list{overflow: hidden; padding-top:90rpx}


.pro_bg{padding: 20rpx; margin: 20rpx; box-shadow:0 0 8px #e2e2e2; -webkit-box-shadow:0 0 8px #e2e2e2; -moz-box-shadow:0 0 8px #e2e2e2; border-radius:5px; -moz-border-radius:5px;}
.pro_bg .pro{position: relative;}
.pro_bg .pro .pic{line-height: 0; font-size: 0;border-radius:8px;overflow: hidden;}
.pro_bg .pro .pic image{width:100%;}

.pro_bg{padding: 20rpx; margin: 20rpx; box-shadow:0 0 8px #e2e2e2; -webkit-box-shadow:0 0 8px #e2e2e2; -moz-box-shadow:0 0 8px #e2e2e2; border-radius:5px; -moz-border-radius:5px;}
.pro_bg .pro{position: relative;}
.pro_bg .pro .pic{line-height: 0; font-size: 0;border-radius:8px;overflow: hidden; position: relative; margin-bottom: 20rpx;}
.pro_bg .pro .pic image{width:100%;}



.pro .text{ width: 100%; height: 80rpx; line-height: 80rpx; position: absolute; left: 0; bottom:0; background:rgba(0, 0, 0, 0.7);}
.pro .quanhao{ line-height:40rpx; font-size: 28rpx; color: #dc0531; }
.pro .text .title{font-size:32rpx; color: #fff; float:left; margin-left: 20rpx;}
.pro .text .box{ color: #fff; line-height:76rpx;font-size:32rpx; float: right; margin-right: 20rpx;}


.pro .txt{ font-size:32rpx; color: #1aad19;}

.pro .right{ position: absolute; right:10rpx; bottom:115rpx;border-radius:4px;overflow: hidden;}
.pro .right .btn{ float: left; font-size:28rpx; padding: 5px 8px;color: #fff; background: #1aad19;}
.pro .right .btn:last-child{ border-left:1px solid #8dd68c;}

.no_data{margin: 400rpx auto 0; text-align: center; font-size:32rpx; color: #666;}

.friend{ overflow: hidden;}
.friend .name{ overflow: hidden; padding-top: 20rpx;}
.friend .name image{ width: 80rpx; border-radius:360px; float: left; display: none;}
.friend .name text{ line-height:70rpx; margin-left: 20rpx; font-size:26rpx; color: #666; display: block; text-align: right;}
.friend .txt{ padding: 10rpx 0 0; color: #333; font-size:26rpx;}

.txt_p{ padding: 10rpx 0;}
.txt_p view{ font-size: 14px; color: #666; padding:5rpx 0;}
.txt_p .txt_p_r{ float: right;}
.txt_bnt{ font-size: 28rpx; padding: 10rpx 20rpx; position: absolute; left: 0px;top: 0px; color: #fff; background: #1aad19;}
.return_view{font-size: 30rpx;color: #1aad19;text-decoration: underline;}
.return_box text{margin:10rpx 0; padding: 10rpx 0 0; color: #333; font-size:26rpx;}

.hint_qrc{ width:300rpx; margin: 0 auto;  height: 280rpx; text-align: center;}
.hint{ border: 1rpx solid #a1afc9;}


.show_detail{ width:250rpx; margin:15rpx 0; line-height:65rpx; font-size:24rpx; color: #666; border: none;position: absolute;right: 0;bottom: 0;}
.show_detail image{ width: 30rpx; height: 30rpx; float: right; margin-top:18rpx;}
.pro_sz_pop{ padding-top: 40rpx;}
.pro_sz_pop .com{ padding-top: 0;margin-bottom: 10rpx;}
.pro_sz_pop .com .text{ text-align: left; height: auto; padding: 20rpx 10rpx 0;color:#DB0050}
.pro_sz_pop .com .text  .sub2 {color: #333;float: left;}
.pro_sz_pop .com .text  .sub3 {float: left;}
.pro_sz_pop .com .text_t{ font-weight: bold; font-size: 35rpx; color: #333;}
.pro_sz_pop .com .text.text_sub{padding:10rpx 0 0 40rpx;font-size: 30rpx;line-height: 40rpx;}
.pro_sz_pop .com .text_title{ height: 65rpx; font-size: 35rpx; font-weight: bold; text-align: center; color: #333;}
.showUse{text-align: right;position: relative;height: 90rpx;}
.pop_com{bottom:100rpx}
 
.ticketBox{overflow: hidden;}
._box{float:left;}
.ticket_date{font-size:28rpx;}
.qrcode{float: right;padding: 10rpx 25rpx;color: #fff;background: #dc0531;border-radius: 28px;font-size: 28rpx;} 
.scan_use .qrc {width:100%; text-align: center;margin:30rpx 0;}
.scan_use .qrc image{width:250rpx; height:250rpx;margin-top: 10rpx; border: 1px solid #c7c7c7;}
.scan_use .item{ width:60%;margin: 0 auto;}
.scan_use .item .box{margin:20rpx 0}
.scan_use .btn{background: #dc0531;color: #fff;border:1px solid #dc0531;border-radius: 28px;font-size: 28rpx;margin: 0 auto; margin-top: 50rpx; width:90%;line-height:80rpx;text-align: center;} 
.scan_use .pro_sz_pop{position: relative;}
.scan_use .ico{position: absolute;top:10rpx; right: 20rpx;font-size: 50rpx;}
.scan_use .pop_com{padding-top: 0rpx;top:20rpx;}

.ticket_date{font-size: 14px;color:#666;margin-top: 10rpx;}